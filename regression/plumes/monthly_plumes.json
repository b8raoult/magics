                "margin_bottom": "10%",
                "margin_top": "20%",
                "plot": { 
                    "nocontour": {
                        "contour_grid_value_plot": "off",
                        "contour": "on",
                        "contour_line_colour": "grey",
                        "contour_label": "off",
                        "contour_level_list": "0.5/10/30/50/120",
                        "contour_level_selection_type": "list",
                        "contour_shade": "on",
                        "contour_highlight": "off",
                        "contour_shade_colour_list": "#FFD073/#FFAA00/#BF8F30/#A66F00",
                        "contour_shade_colour_method": "list",
                        "contour_shade_method": "area_fill"
                    },
                    "epsplume": { 
                        "eps_plume_members":"off",
                        "eps_plume_median":"on",
                        "eps_plume_median_line_thickness":"5",
                        "eps_plume_median_line_colour":"#cd0174",
                        "eps_plume_forecast":"off",
                        "eps_plume_control":"off",
                        "eps_plume_shading":"on",
                        "eps_plume_shading_level_list":"0/12.5/25/37.5/62.5/75/87.5/100",
                        "eps_plume_shading_colour_list":"#d0fd74/#9fee01/#789837/#4f7600"
					},
                    "wrepjson": {
                        "wrepjson_family": "eps",
